            <string notr="true">QDateEdit {
    border: 2px solid #2980b9;
    border-radius: 10px;
    padding: 3px 12px;
    background-color: white;
}
/* Vista de días (el grid del calendario) */
QDateEdit QAbstractItemView {
    background-color: white;
    color: #2d2d2d;
    selection-background-color: #2980b9;
    selection-color: white;
}
/* Botones de navegación (mes anterior/siguiente) y combos de mes/año */
QCalendarWidget QWidget#qt_calendar_navigationbar {
    background-color: #90caf9;   /* Fondo del encabezado */
}

QCalendarWidget QToolButton {
    color: #2d2d2d;              /* Texto de mes y año */
    background-color: transparent;
    font-weight: bold;
    border: none;
    padding: 4px 8px;
}

QCalendarWidget QToolButton:hover {
    background-color: #d6eaff;   /* Hover sobre mes/año o flechas */
    border-radius: 4px;
}

/* Hover sobre los días */
QDateEdit QAbstractItemView::item:hover {
    background-color: #2980b9;   /* azul claro */
    color: #000;
}
